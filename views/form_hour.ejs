<form action="/createBusyHour" class="form-group" id="formBusyHour">
<div class="row d-flex p-2">
      <div class="form-group" class="col-sm-3">
        <label>Day:</label>
            <select onclick="updateDay()" id="controlSelectDay" name="day" multiple class="form-control" >
            <option value="Monday">Monday</option>
            <option value="Tuesday">Tuesday</option>
            <option value="Wednesday">Wednesday</option>
            <option value="Thursday">Thursday</option>
            <option value="Friday">Friday</option>
            <option value="Saturday">Saturday</option>
            <option value="Sunday">Sunday</option>
            </select>
      </div>
      <div class="form-group" class="col-sm-3">
        <label>Start hour:</label>
        <select onclick="updateStartHour()" id="controlSelectS" name="start" multiple class="form-control" >
        <% hours.forEach( (item) => { %>
        <option value=<%- "'" + item + "'" %>> <%= item %></option>
        <% }) %>
        </select>
      </div>
      <div class="form-group" class="col-sm-3">
        <label>End hour:</label>
        <select onclick="updateEndHour()" id="controlSelectE" name="end" multiple class="form-control" >
        <% hours.forEach( (item) => { %>
        <option><%= item %></option>
        <% }) %>
        })
        </select>
      </div>
    </form>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</div>